/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "paths": {
      "core-js/es7/reflect": ["./node_modules/core-js/proposals/reflect-metadata"],
      "core-js/es6/*": ["./node_modules/core-js/es/*"]
    },
    "outDir": "./out-tsc/app",
    "types": [],
    "strict":false,
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
